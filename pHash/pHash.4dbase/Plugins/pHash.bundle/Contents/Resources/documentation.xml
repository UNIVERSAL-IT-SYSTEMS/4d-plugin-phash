<?xml version="1.0" encoding="UTF-8" standalone="no" ?><ex4D:plugin xmlns:ex4D="http://www.4d.com/plugin">  <!--pHash-->  <ex4D:documentation name="pHash" plugin-id="20000">    <ex4D:plugin-description>      <!-- write documentation for pHash here -->	  	  <p>This plugin is a 4D implementation of the <a href="http://www.phash.org">pHash</a> open source perceptual hash library.</p>	  <p>It can compute and compare the perceptual hashes of PNG, BMP and JPEG files.</p>	  <p>You can use it, for example, to search similar images of different file types stored in your database.</p>	  <p>Note: Audio and Video files are not supported.</p>	  <p>Note: If you create the PNG on Mac OS, make sure the &quot;simple file&quot; option is NOT used. Otherwise the hash value will be zero.</p>	      </ex4D:plugin-description>    <ex4D:commands>      <ex4D:command-group command-group-id="1" name="Compute">        <ex4D:command command-id="1" name="PH Compute DCT">          <ex4D:command-description>            <!-- write documentation for PH Compute DCT here -->          </ex4D:command-description>          <ex4D:argument argument-id="1" label="path" type="T">            <!-- write documentation for $1 (TEXT) here-->          </ex4D:argument>          <ex4D:argument argument-id="2" label="hash" type="T">            <!-- write documentation for $2 (TEXT) here-->			<p>String representation of unsigned 64 bit integer hash.</p>          </ex4D:argument>          <ex4D:return-value label="status" type="L">            <!-- write documentation for return value (LONGINT) here-->			<p>Less than 0 for error.</p>          </ex4D:return-value>        </ex4D:command>      </ex4D:command-group>      <ex4D:command-group command-group-id="2" name="Compare">        <ex4D:command command-id="2" name="PH Compare MH">          <ex4D:command-description>            <!-- write documentation for PH Compare MH here -->          </ex4D:command-description>          <ex4D:argument argument-id="1" label="pathA" type="T">            <!-- write documentation for $1 (TEXT) here-->          </ex4D:argument>          <ex4D:argument argument-id="2" label="pathB" type="T">            <!-- write documentation for $2 (TEXT) here-->          </ex4D:argument>          <ex4D:argument argument-id="3" label="alpha" type="8">            <!-- write documentation for $3 (REAL) here-->			<p>Scale factor for marr wavelet.</p>          </ex4D:argument>          <ex4D:argument argument-id="4" label="level" type="8">            <!-- write documentation for $4 (REAL) here-->			<p>Level of scale factor.</p>          </ex4D:argument>          <ex4D:return-value label="hammingDistance" type="8">            <!-- write documentation for return value (REAL) here-->			<p>Normalized hamming distance between the two hashes.</p>          </ex4D:return-value>        </ex4D:command>        <ex4D:command command-id="3" name="PH Compare RADISH">          <ex4D:command-description>            <!-- write documentation for PH Compare RADISH here -->          </ex4D:command-description>          <ex4D:argument argument-id="1" label="pathA" type="T">            <!-- write documentation for $1 (TEXT) here-->          </ex4D:argument>          <ex4D:argument argument-id="2" label="pathB" type="T">            <!-- write documentation for $2 (TEXT) here-->          </ex4D:argument>          <ex4D:argument argument-id="3" label="sigma" type="8">            <!-- write documentation for $3 (REAL) here-->			<p>Deviation for a gaussian filter function.</p>          </ex4D:argument>          <ex4D:argument argument-id="4" label="gamma" type="8">            <!-- write documentation for $4 (REAL) here-->			<p>Gamma correction on the input image.</p>          </ex4D:argument>          <ex4D:argument argument-id="5" label="N" type="L">            <!-- write documentation for $5 (LONGINT) here-->			<p>Number of angles to consider.</p>          </ex4D:argument>          <ex4D:return-value label="pcc" type="8">            <!-- write documentation for return value (REAL) here-->			<p>Peak of cross correlation.</p>          </ex4D:return-value>        </ex4D:command>        <ex4D:command command-id="4" name="PH Compare DCT">          <ex4D:command-description>            <!-- write documentation for PH Compare DCT here -->          </ex4D:command-description>          <ex4D:argument argument-id="1" label="hashA" type="T">            <!-- write documentation for $1 (TEXT) here-->			<p>String representation of unsigned 64 bit integer hash.</p>			          </ex4D:argument>          <ex4D:argument argument-id="2" label="hashB" type="T">            <!-- write documentation for $2 (TEXT) here-->			<p>String representation of unsigned 64 bit integer hash.</p>			          </ex4D:argument>          <ex4D:return-value label="hammingDistance" type="L">            <!-- write documentation for return value (LONGINT) here-->			<p>Hamming distance between the two hashes.</p>			          </ex4D:return-value>        </ex4D:command>      </ex4D:command-group>    </ex4D:commands>  </ex4D:documentation></ex4D:plugin>